<tool id="d3_network" name="Force directed d3 network">
  <description>Creates d3 force directed network from pickle file</description>
  <command interpreter="python">force_directed.py $pickle > $dummyout
  </command>
  <inputs>
    <param name="pickle" type="data" label="Pickle"/>
  </inputs>

  <outputs>
    <data format="html" name="dummyout" />
  </outputs>

  <help>
    force_directed.py --pickle pickle_file
  </help>
</tool>
